ALTER PROCEDURE View_309Approval_Status
    @Doc_ID INT
AS
BEGIN
    SELECT 
        Trn_Appl_His_ID,
        Approver_Status As Status,
        Approver_Comment,
        Modified_By,
        Modified_On
    FROM Trn_Approval_History
    WHERE 
        Doc_ID =  @Doc_ID
        AND Approver_Status='Approved'
        
    ORDER BY Modified_On DESC
END
